							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"userId\": {{userId}},\n  \"username\": \"{{username}}\",\n  \"entityType\": \"ASSET\",\n  \"entityId\": {{assetId}},\n  \"targetUserId\": {{targetUserId}},\n  \"targetUsername\": \"{{targetUsername}}\"\n}",
