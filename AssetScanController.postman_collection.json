					"raw": "{\n    \"scanValue\": \"SN123456\",\n    \"scanType\": \"BARCODE\",\n    \"userId\": {{userId}},\n    \"username\": \"{{username}}\",\n    \"projectType\": \"{{projectType}}\",\n    \"assetNameUdv\": \"Laptop-001\",\n    \"serialNumber\": \"SN123456\",\n    \"categoryName\": \"Electronics\",\n    \"subCategoryName\": \"Laptops\",\n    \"makeName\": \"Dell\",\n    \"modelName\": \"XPS 15\",\n    \"assetStatus\": \"ACTIVE\",\n    \"purchaseDate\": \"2024-01-15\",\n    \"categoryId\": {{categoryId}},\n    \"subCategoryId\": {{subCategoryId}},\n    \"makeId\": {{makeId}},\n    \"modelId\": {{modelId}}\n}",
					"options": {
